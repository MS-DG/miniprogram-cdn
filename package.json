{
    "name": "miniprogram-cdn",
    "version": "0.0.0",
    "description": "auzre cn cdn compress for miniprogarm",
    "main": "dist/index.js",
    "module": "es/index.js",
    "typings": "es/index.d.ts",
    "files": [
        "dist/",
        "es/"
    ],
    "scripts": {
        "start": "npm run build",
        "build": "npm run build:es5 && npm run build:es6",
        "build:es5": "tsc index.ts --target es5 -d --outDir dist",
        "build:es6": "tsc index.ts --target es6 -m es6 -d --outDir es",
        "pub": "npm run build && npm publish --access public",
        "test": "npm run build"
    },
    "author": "New Future <New.Future@microsfot.com>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MS-DG/miniprogram-cdn.git"
    },
    "license": "ISC",
    "devDependencies": {
        "typescript": "^3.3.3333"
    }
}
